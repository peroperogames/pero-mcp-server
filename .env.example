# ========================================
# SSH Client Configuration
# ========================================

# SSH服务器基本配置
SSH_HOST=your-ssh-host-ip-or-domain
SSH_USERNAME=your-ssh-username
SSH_PORT=22
SSH_TIMEOUT=30
SSH_PASSWORD=your-ssh-password

# ========================================
# App Store Connect Configuration
# ========================================

# App Store Connect API配置
APPSTORE_ISSUER_ID=your-issuer-id
APPSTORE_KEY_ID=your-key-id
APPSTORE_PRIVATE_KEY=-----BEGIN PRIVATE KEY-----\nyour-private-key-content\n-----END PRIVATE KEY-----
APPSTORE_VENDOR_NUMBER=your-vendor-number

# ========================================
# Google Play Developer Reporting API Configuration
# ========================================

# Google Play服务账号配置（从Google Cloud Console获取）
GOOGLE_PLAY_SERVICE_ACCOUNT_TYPE=service_account
GOOGLE_PLAY_SERVICE_ACCOUNT_PROJECT_ID=your-project-id
GOOGLE_PLAY_SERVICE_ACCOUNT_PRIVATE_KEY_ID=your-private-key-id
GOOGLE_PLAY_SERVICE_ACCOUNT_PRIVATE_KEY=-----BEGIN PRIVATE KEY-----\nyour-service-account-private-key\n-----END PRIVATE KEY-----
GOOGLE_PLAY_SERVICE_ACCOUNT_CLIENT_EMAIL=your-service-account@your-project.iam.gserviceaccount.com
GOOGLE_PLAY_SERVICE_ACCOUNT_CLIENT_ID=your-client-id
GOOGLE_PLAY_SERVICE_ACCOUNT_AUTH_URI=https://accounts.google.com/o/oauth2/auth
GOOGLE_PLAY_SERVICE_ACCOUNT_TOKEN_URI=https://oauth2.googleapis.com/token
GOOGLE_PLAY_SERVICE_ACCOUNT_AUTH_PROVIDER_X509_CERT_URL=https://www.googleapis.com/oauth2/v1/certs
GOOGLE_PLAY_SERVICE_ACCOUNT_CLIENT_X509_CERT_URL=https://www.googleapis.com/robot/v1/metadata/x509/your-service-account%40your-project.iam.gserviceaccount.com
GOOGLE_PLAY_SERVICE_ACCOUNT_UNIVERSE_DOMAIN=googleapis.com

# 可选：特定应用包名
GOOGLE_PLAY_PACKAGE_NAME=com.yourcompany.yourapp

# Google Play云存储和财务报告配置
GOOGLE_PLAY_CLOUD_STORAGE_BUCKET=your_bucket